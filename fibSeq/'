#include <stdio.h>

int main() {
    int num;
    while (1) {
        printf("Enter a number: ");

        if (scanf(" %d", &num) != 1) {
            fprintf(stderr, "Invalid input. Please enter an integer\n");
            while (getchar() != '\n');
            continue;
        }

        printf("%d", 0);
        unsigned long long long num1 = 0, num2 = 1, temp;
        for (int i = 0; i < num - 1; i++) {
            printf(", %lld", num2);
            temp = num2;
            num2 += num1;
            num1 = temp;
        }
        printf("\n");
    }

    return 0;
}
